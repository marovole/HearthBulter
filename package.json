{
  "name": "health-butler",
  "version": "0.2.0",
  "private": true,
  "description": "基于健康数据与电商库存的动态饮食引擎",
  "author": "Ronn Huang",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "build:cloudflare": "prisma generate && next build && npx @opennextjs/cloudflare build && cp -f .open-next/worker.js .open-next/_worker.js && cp -f wrangler.toml .open-next/wrangler.toml && node scripts/add-compat-flags.js && node scripts/create-node-stubs.js && node scripts/fix-cloudflare-imports.js && node scripts/fix-prisma-bundle.js",
    "build:cloudflare-optimized": "pnpm run optimize-deps && pnpm run build:cloudflare && pnpm run check-bundle-size",
    "build:cloudflare-hybrid": "bash scripts/build-cloudflare-hybrid.sh",
    "optimize-deps": "node scripts/optimize-dependencies.js",
    "check-bundle-size": "node scripts/check-bundle-size.js",
    "deploy": "pnpm cloudflare:deploy",
    "deploy:prod": "pnpm cloudflare:deploy production",
    "deploy:cloudflare": "pnpm run build:cloudflare && wrangler pages deploy .open-next --project-name=hearthbulter",
    "deploy:cloudflare-hybrid": "bash scripts/deploy-cloudflare-hybrid.sh",
    "check:deployment": "./scripts/check-deployment.sh",
    "quick-status": "./scripts/quick-status-check.sh",
    "setup:env": "./scripts/setup-production-env.sh",
    "setup:supabase": "./scripts/setup-supabase.sh",
    "deploy:database": "./scripts/deploy-database.sh",
    "quick-fix": "./scripts/quick-fix-check.sh",
    "restore:supabase": "./scripts/restore-supabase.sh",
    "emergency:fix": "./scripts/emergency-supabase-fix.sh",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "security:check": "npm audit && npm outdated",
    "pre-deploy": "tsx scripts/pre-deploy-check.ts",
    "review": "node scripts/pre-commit-code-review.js",
    "review:report": "node scripts/generate-review-report.js",
    "review:report:html": "node scripts/generate-review-report.js --html",
    "quality:scan": "node scripts/code-quality-improvements.js",
    "quality:fix": "node scripts/fix-common-issues.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:test": "tsx scripts/test-db-connection.ts",
    "test:ocr-setup": "tsx src/lib/services/test-ocr-setup.ts",
    "supabase:generate-schema": "tsx scripts/generate-supabase-schema.ts",
    "supabase:migrate-data": "tsx scripts/migrate-data-to-supabase.ts",
    "supabase:test": "node scripts/test-supabase-connection.js",
    "cloudflare:deploy": "./scripts/deploy-cloudflare-supabase.sh",
    "cloudflare:deploy:staging": "./scripts/deploy-cloudflare-supabase.sh staging",
    "cloudflare:deploy:dev": "./scripts/deploy-cloudflare-supabase.sh development",
    "prepare": "husky install",
    "postinstall": "prisma generate",
    "openspec:list": "openspec list",
    "openspec:validate": "openspec validate --strict"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@prisma/client": "^6.0.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/supabase-js": "^2.80.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^7.0.3",
    "@upstash/redis": "^1.35.6",
    "axios": "^1.13.1",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.24",
    "html2canvas": "^1.4.1",
    "isomorphic-dompurify": "^2.31.0",
    "lucide-react": "^0.460.0",
    "next": "^15.0.0",
    "next-auth": "^4.24.13",
    "next-themes": "^0.4.6",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.10",
    "openai": "^6.7.0",
    "pdf-parse": "^2.4.5",
    "puppeteer": "^24.27.0",
    "qrcode": "^1.5.3",
    "react": "^18.3.1",
    "react-day-picker": "^9.11.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-intersection-observer": "^10.0.0",
    "react-is": "^19.2.0",
    "recharts": "^3.3.0",
    "sharp": "^0.34.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.5.0",
    "tesseract.js": "^6.0.1",
    "zod": "^3.23.8",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.0.0",
    "@opennextjs/cloudflare": "^1.11.0",
    "@playwright/test": "^1.48.0",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.0",
    "prettier": "^3.3.0",
    "prettier-plugin-tailwindcss": "^0.6.0",
    "prisma": "^6.0.0",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "tsx prisma/seed.ts"
  },
  "optionalDependencies": {
    "ioredis": "^5.3.2"
  },
  "cloudflare": {
    "exclude": [
      "puppeteer",
      "@sparticuz/chromium",
      "chrome-aws-lambda",
      "puppeteer-core",
      "@prisma/client/runtime/query_engine_bg.mysql.wasm",
      "@prisma/client/runtime/query_engine_bg.sqlite.wasm",
      "@prisma/client/runtime/query_engine_bg.sqlserver.wasm",
      "@prisma/client/runtime/query_engine_bg.cockroachdb.wasm",
      "@prisma/client/runtime/query_compiler_bg.mysql.wasm",
      "@prisma/client/runtime/query_compiler_bg.sqlite.wasm",
      "@prisma/client/runtime/query_compiler_bg.sqlserver.wasm",
      "@prisma/client/runtime/query_compiler_bg.cockroachdb.wasm"
    ],
    "optimize": true
  }
}